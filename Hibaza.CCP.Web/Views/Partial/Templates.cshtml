<div class="lg-partial_templates">
    <script type="text/html" class="js-loadtemplate-ticket">
        <li style="${hidden}" data-status="${status}" data-responsability="${responsability}" data-agent="${agent}" data-key="${key}" onclick="cloud.getTicketProfile(${key});viewProfileTicket();" class="list-group-item wz-flex-card js-ticket">
            <!--class active-->
            <div class="wz-flex-card-item">
                <img src="/avatars/${phone}.jpg" alt="" WIDTH="40" class="img-circle" />
            </div>
            <div class="wz-flex-card-item js-ticket-search-field">
                <p class="wz-font-w-med">${description}</p>
                <p class="wz-font-w-med wz-font-s-sm">
                    <span class="wz-client">${customer}</span><span class="wz-date">${date}</span>
                </p>
            </div>
            <div class="wz-flex-card-item">
                <div href="" onclick="" class="btn btn-info">
                    ${status_display}
                </div><br />
                <div href="" onclick="" class="btn btn-info">
                    ${responsability_display}
                </div>
            </div>
        </li>
    </script>
    <script type="text/html" class="js-loadtemplate-contact">
        <li timestamp="${date}" data-key="${key}" onclick="firebaseService.setMainChat('${key}'); viewProfileClient(); cloud.getLastTicket('${key}'); getCustomerAi('${id}', '${facebook_id}');" data-agent="${assigned}" data-status="${status}" data-selector="${selector}" class="list-group-item wz-flex-card js-contact-${key} ${unread} ${archived}">
            <div class="wz-flex-card-item">
                <div title="${name}" style="background-image:url(${picture})" class="wz-contact-avatar js-picture-${key} ${avatar_img}"></div>
                <div title="${name}" class="wz-contact-avatar js-avatar-${key} ${avatar_div}">
                    <p>${initials}</p>
                </div>
            </div>
            <div class="wz-flex-card-item js-contacts-search-field">
                <p class="wz-font-w-med">
                    <span class="wz-contact-name js-name-${key}">${name}</span><i class="js-channel-icon ${unread} icon-${channel_ico}"></i>
                </p>
                <p class="${unread} wz-font-s-sm js-last-message-${key}">${last_message}</p>
                <p class="wz-font-w-reg wz-font-s-sm">
                    <span class="wz-date js-date-${key}"><span class="js-channel-name wz-padding-r-5 ${channel_status}">${channel_name}</span><span class="js-render-date">${render_date}</span></span>
                </p>
            </div>
            <div class="wz-flex-card-item">
                <div href="" onclick="${assigned_function}" class="js-status ${btn_class}">${assigned_text}</div>
            </div>
        </li>
    </script>
    <script type="text/html" class="js-loadtemplate-conversation">
