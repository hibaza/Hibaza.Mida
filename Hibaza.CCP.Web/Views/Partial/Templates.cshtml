                <p class="wz-font-w-med">
                    <span class="wz-contact-name  js-name-${key} ">${name}</span>
                </p>
            </div>
            <div class="wz-flex-card-item">
                <div href="" onclick="${assigned_function}" class="js-status ${btn_class}">${assigned_text}</div>
            </div>
        </li>
    </script>

