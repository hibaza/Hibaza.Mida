    <script type="text/html" class="js-loadtemplate-message">
        <li data-key="${key}" data-day="${day}" data-timestamp="${timestamp}" data-author="${author_id}" data-agent="${agent}" class="list-group-item wz-flex-card js-chat-message ${dayClass}  ${deleted}">

            <div class="wz-flex-card-item">
                <a href="${author_url}" target="_blank">
                    <div style="background-image: url(${author_picture})" class="wz-message-avatar js-picture-${author_key} ${avatar_img}"></div>
                </a>
                <div class="wz-message-avatar js-avatar-${author_key} ${avatar_div}">
                    <p>${initials}</p>
                </div><span onclick="firebaseService.starMessage('${key}')" data-toggle="tooltip" title="Star this message" data-placement="top" data-container="body" class="wz-starred group ${starred}"></span>
            </div>
            <div class="wz-flex-card-item">
                <a href="${author_url}" target="_blank"><span class="wz-font-w-med wz-caption js-name-${author_key}">${author_name}</span></a>
                <span class="wz-font-w-reg wz-caption wz-font-s-sm">
                    <a href="${permalink}" target="_blank">
                        ${date}
                    </a>
                    <span style="background-image:url(${readed_ico})" class="wz-checks ${readed}"></span>
                    <a href="#" class="wz-margin-l-5 ${hidden} lg-reply" onclick="phoneInput.val('${key}'), txt.focus()">
                        Reply
                    </a>
                    <a href="#" class="wz-margin-l-5 ${hidden}" onclick="firebaseService.likeMessage('${key}', this)">
                        ${like}
                    </a>
                    <span style="background-image:url(/images/forward.png) ;width: 17px; height: 11px;background-repeat: no-repeat;" onclick="editToBot('${message}')" class="wz-checks wz-checks-1"></span>
                    <span onclick="firebaseService.starMessage('${key}')" data-toggle="tooltip" title="Star this message" data-placement="top" data-container="body" class="wz-starred ${starred}"></span>

                </span>
                <pre class="js-chat-message-text">${message}
      </pre>
            </div>
        </li>
    </script>

