        <li data-key="${key}" data-day="${day}" data-timestamp="${timestamp}" data-author="${author_id}" data-agent="${agent}" class="list-group-item wz-flex-card js-chat-zami js-chat-message ${dayClass}">
            <div class="wz-flex-card-item">
                <img src="${author_picture}" alt="" WIDTH="40" class="js-picture-${author_key}" />
            </div>
            <div class="wz-flex-card-item">
                <div class="wz-message-text">
                    <span class="wz-font-w-med wz-caption"><span class="wz-font-c-pink">${author_name}</span></span><span class="wz-font-w-reg wz-caption wz-font-s-sm">${date}</span>
                    <div class="wz-container ${template}">
                        <pre class="js-chat-message-text js-chat-zami-text wz-zami-note">${title}
          </pre>
                        <div class="wz-button-container"></div>
                    </div>
                </div>
            </div>
        </li>
    </script>
    <script type="text/html" class="js-loadtemplate-notification">
        <li>
            <a href="#">
                <p class="wz-notification-date wz-font-s-sm">
                    <i class="icon-clock"></i>${date}
                </p>
                <p class="wz-notification-author">
                    <i class="icon-tag"></i>${tag}
                </p>
                <p class="wz-notification-title wz-font-w-med wz-font-s-sm wz-font-u-uppercase wz-margin-0">
                    <i class="icon-paper"></i>${message}
                </p>
            </a>
        </li>
    </script>
    <script type="text/html" class="js-loadtemplate-disconnection">
        <div class="alert alert-warning fade in">
            <a href="#" data-dismiss="alert" aria-label="close" title="Close" class="close">×</a><strong class="lg-connection-active">Your connection is not active.</strong><span class="lg-trying-reconnect">Trying to reconnect with Hibaza...</span>
        </div>
    </script>
    <script type="text/html" class="js-loadtemplate-ghost">
        <li class="list-group-item wz-flex-card js-chat-message js-chat-ghost">
            <div class="wz-flex-card-item">
                <div style="background-image: url(${avatar})" class="wz-message-avatar"></div>
            </div>
            <div class="wz-flex-card-item">
                <span class="wz-font-w-med">${name}</span><span class="wz-font-w-reg wz-font-s-sm">${date}</span><span style="background-image:url(/images/gray-check.png)" class="wz-checks wz-checks-1"></span>
                <p class="js-chat-message-text">
                    ${message}
                </p>
                <div class="js-chat-ghost-action">
                    <i class="icon-zami-07"></i><span class="lg-message-not-sent">Message not sent, would you like to </span><a href="#" class="js-resend-message lg-resend">resend</a> or  <a href="#" class="js-remove-message lg-remove">remove</a>?
                </div>
            </div>
        </li>
    </script>

    <script type="text/html" class="js-loadtemplate-trash">
        <div class="dropdown wz-trash pull-right">
            <span onclick="confirmMsgDel(this, '${key}')" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" class="wz-trash-btn wz-font-s-sm wz-font-c-dark"><i data-toggle="tooltip" title="Delete this message" data-placement="top" data-container="body" class="icon-trash"></i></span>
        </div>
    </script>
    <script type="text/html" class="js-loadtemplate-delconfirm">
        <ul class="js-delconfirm dropdown-menu dropdown-menu-right">
            <li class="dropdown-header">
                <h6 class="lg-are-you-sure">
                    Are you sure?
                </h6>
                <p class="wz-font-u-uppercase wz-font-s-xs lg-cannot-recovered">
                    The message cannot be recovered
                </p>
            </li>
            <li class="text-center">
                <div class="btn-group">
                    <a href="#" data-toggle="dropdown" role="button" class="js-cancel btn btn-default btn-xs"><span class="wz-font-s-xs wz-font-w-med wz-font-u-uppercase wz-font-c-dark lg-cancel">Cancel</span></a><a onclick="cloud.deleteMessage($(this),'${key}')" data-toggle="tooltip" title="Star this message" data-placement="top" data-container="body" class="js-delete btn btn-pink btn-xs"><span class="wz-font-s-xs wz-font-w-med wz-font-u-uppercase wz-font-c-white lg-delete">Delete</span></a>
                </div>
            </li>
        </ul>
    </script>
