            <!--class active ...djfd ${channel_ico}-->
            <div class="wz-flex-card-item" style="font-size:25px; padding-top:10px">
                <i class="js-channel-icon icon-${channel_ico}"></i>
            </div>
            <div class="wz-flex-card-item js-contacts-search-field">
